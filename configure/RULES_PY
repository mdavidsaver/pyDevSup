ifneq ($(T_A),)
ifndef PY_VER
$(error Must include CONFIG_PY)
endif

PY_FILES += $(PY:%=$(PY_INSTALL_DIR)/%)

$(PY_FILES) : $(PY_INSTALL_DIR)/%: ../%
ifneq ($(OS),Windows_NT)
	@[ -d $(dir $@) ] || install -d $(dir $@)
	@echo "Install PY $@"
	install -m 644 $< $@
else
	xcopy /S /Q /Y /F $(subst /,\,$<) $(subst /,\,$@)*
endif

build: $(PY_FILES)

endif
